{
  "name": "spec-tracker-error",
  "version": "1.0.0",
  "description": "Boilerplate Node.JS + TypeScript + DynamoDB",
  "main": "dist/server.js",
  "author": "Douglas Dennys <douglasdennys45@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "build:watch": "rimraf dist && tsc -p tsconfig-build.json -w",
    "check": "npm-check -s -u",
    "test": "jest --passWithNoTests --runInBand --no-cache --detectOpenHandles",
    "test:verbose": "jest --passWithNoTests --runInBand --forceExit",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests --forceExit",
    "test:ci": "npm test -- --coverage --forceExit"
  },
  "keywords": [
    "Node.JS",
    "TypeScript",
    "DDD",
    "SOLID",
    "TDD",
    "Clean Architecture",
    "Clean Code",
    "MongoDB"
  ],
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.6",
    "restana": "^4.8.1",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.4",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.0",
    "@types/jest": "^26.0.20",
    "@types/mongodb": "^3.6.12",
    "@types/node": "^14.14.28",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "faker": "^5.5.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.5"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
